{
  "name": "contributte/turnstile",
  "description": "CloudflareTurnstile for Nette Framework",
  "keywords": ["nette", "turnstile", "captcha"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/turnstile",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.3",
    "nette/forms": "^3.3.0"
  },
  "require-dev": {
    "contributte/phpstan": "^0.2.0",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Turnstile\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
